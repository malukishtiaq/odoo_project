<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="inherit_account_account_form_view" model="ir.ui.view">
        <field name="name">account.account.form.internal.transfer</field>
        <field name="model">account.account</field>
        <field name="inherit_id" ref="account.view_account_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='allowed_journal_ids']" position="after">
                    <field name="is_internal_transfer_account"/>
            </xpath>
        </field>
    </record>
</odoo>