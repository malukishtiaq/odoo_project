<?xml version="1.0" encoding="UTF-8"?>
<templates id="point_of_sale.template" xml:space="preserve">

        <t t-name="Navbar" t-inherit="point_of_sale.Navbar" t-inherit-mode="extension" owl="1">
            <xpath expr="//div[hasclass('pos-topheader')]/div[hasclass('pos-centerheader')]/img[hasclass('pos-logo')]"
                   position="replace">
                <img class="pos-logo pos-logo mw-100" height="42" t-on-click="() => debug.toggleWidget()"
                     src="/gl_pos_logo/static/src/img/logo.png" alt="Logo"/>
            </xpath>
        </t>

</templates>